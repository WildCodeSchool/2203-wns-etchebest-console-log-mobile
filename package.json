{
  "name": "console-log-app",
  "version": "0.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npm-run-all --parallel codegen:watch expo:start",
    "android": "npm-run-all --parallel codegen:watch expo:android",
    "ios": "npm-run-all --parallel codegen:watch expo:ios",
    "web": "npm-run-all --parallel codegen:watch expo:web",
    "build:staging": "npm run codegen && eas build -p android --profile staging --non-interactive",
    "build:production": "npm run codegen && eas build -p android --profile production --non-interactive",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier . --ignore-path .eslintignore --write",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "npx graphql-codegen --watch",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/core": "^6.4.5",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/material-top-tabs": "^6.2.4",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.1",
    "@testing-library/react-native": "11.0.0",
    "dotenv": "^16.0.2",
    "expo": "^47.0.0",
    "expo-constants": "~14.0.2",
    "expo-font": "~11.0.1",
    "expo-image-picker": "~14.0.2",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "graphql": "^15.8.0",
    "jest": "^26.6.3",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.36.1",
    "react-native": "0.70.5",
    "react-native-dropdown-picker": "^5.4.2",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-pager-view": "6.0.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-web": "~0.18.7",
    "react-test-renderer": "18.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/client-preset": "1.2.3",
    "@graphql-codegen/introspection": "2.2.3",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.8.0",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx,json}": [
      "prettier --write"
    ]
  },
  "private": true
}
